[user]
	email = 
	name = 
	signingKey = 
[alias]
	aliases = config --get-regexp alias
	reuse = commit --reedit-message=HEAD --reset-author
	fastamend = commit --amend --no-edit --reset-author
	st = status
	ch = checkout
	br = branch
	list = log --oneline
	graph = log --graph --decorate --pretty=oneline --abbrev-commit --all
[push]
	autoSetupRemote = true
[gpg]
	format = ssh
[commit]
	gpgSign = true

